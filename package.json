{
  "name": "orchestra-components",
  "description": "Orchestra Component Library",
  "version": "0.1.0",
  "author": {
    "name": "Jonas Schwendener",
    "email": "jonas.schwendener@gmail.com"
  },
  "keywords": [
    "vue",
    "component",
    "library",
    "orchestra"
  ],
  "private": false,
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name OrchestraComponents src/components/index.js",
    "lint": "vue-cli-service lint"
  },
  "main": "./dist/OrchestraComponents.umd.js",
  "unpkg": "dist/OrchestraComponents.umd.min.js",
  "files": [
    "dist/*",
    "src/*"
  ],
  "dependencies": {
    "core-js": "^3.3.2",
    "flatpickr": "^4.6.3",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
